{
    "name": "timeline-planner",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "run-p dev:client dev:server",
        "dev:client": "vite --config client/vite.config.js",
        "dev:server": "tsx --tsconfig server/tsconfig.json server/src/index.ts",
        "build": "run-p type-check build:client build:server",
        "build:client": "vite build --config client/vite.config.js",
        "build:server": "tsc --project server/tsconfig.json",
        "start:server": "node dist/server/src/index.js",
        "type-check": "run-p type-check:client type-check:server",
        "type-check:client": "vue-tsc --project client/tsconfig.json --noEmit",
        "type-check:server": "tsc --project server/tsconfig.json --noEmit",
        "lint": "eslint",
        "lint-fix": "eslint --fix",
        "preview": "vite preview --config client/vite.config.js"
    },
    "dependencies": {
        "@primeuix/themes": "^1.1.1",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "pinia": "^3.0.2",
        "primeicons": "^7.0.0",
        "primevue": "^4.3.4",
        "tailwindcss-primeui": "^0.6.1",
        "uuid": "^9.0.1",
        "vue": "^3.5.14",
        "zod": "^3.25.28"
    },
    "devDependencies": {
        "@primevue/auto-import-resolver": "^4.3.4",
        "@tailwindcss/postcss": "^4.1.7",
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.2",
        "@types/node": "^22.15.21",
        "@types/uuid": "^9.0.8",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.20",
        "ccs-lint": "1.0.19",
        "eslint": "^9.27.0",
        "eslint-import-resolver-vite": "^2.1.0",
        "globals": "^16.2.0",
        "npm-run-all": "^4.1.5",
        "tailwindcss": "^4.1.7",
        "tsx": "^4.19.4",
        "typescript": "^5.8.2",
        "vite": "^6.2.0",
        "vue-tsc": "^2.2.10"
    }
}
